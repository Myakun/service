    "kartik-v/yii2-bootstrap5-dropdown": "v1.0.2",
    "kartik-v/yii2-field-range": "v1.3.5",
    "kartik-v/yii2-grid": "v3.5.0",
    "kartik-v/yii2-nav-x": "v1.2.5",
    "kartik-v/yii2-widgets": "v3.4.1",
    "kartik-v/yii2-widget-typeahead": "v1.0.4",
    "kotchuprik/yii2-sortable-widgets": "v2.1.0",
    "myakun/yii2-recaptcha-widget" : "*",
    "php": ">=8.1",
    "yiisoft/yii2": "2.0.45",
    "yiisoft/yii2-bootstrap5": "*",
    "yiisoft/yii2-debug": "2.1.19",
    "yiisoft/yii2-swiftmailer": "2.1.3",
    "yii2tech/ar-position": "1.0.1",
    "ext-mbstring": "*"
