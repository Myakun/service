{
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/Myakun/yii2-recaptcha-widget.git"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "components/font-awesome": "6.1.0",
